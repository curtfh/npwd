{
  "PhoneAsItem": {
    "enabled": true,
    "exportResource": "aurora_npwd",
    "exportFunction": "doesClientHavePhone"
  },
  "customPhoneNumber": {
    "enabled": true,
    "exportResource": "aurora_npwd",
    "exportFunction": "generateNumber"
  },
  "general": {
    "useResourceIntegration": true,
    "toggleKey": "f3",
    "toggleCommand": "phone",
    "defaultLanguage": "en",
    "showId": false
  },
  "database": {
    "useIdentifierPrefix": true,
    "playerTable": "users",
    "identifierColumn": "identifier",
    "identifierType": "steam",
    "profileQueries": true,
    "phoneNumberColumn": "phone"
  },
  "images": {
    "url": "https://aurorahub.gg/rpp/imgs/upload.php",
    "type": "pe_image",
    "imageEncoding": "webp",
    "contentType": "multipart/form-data",
    "useContentType": false,
    "useWebhook": false,
    "authorizationHeader": "PE-Secret",
    "authorizationPrefix": "",
    "useAuthorization": false,
    "returnedDataIndexes": ["url"]
  },
  "imageSafety": {
    "filterUnsafeImageUrls": true,
    "embedUnsafeImages": false,
    "embedUrl": "https://i.example.com/embed",
    "safeImageUrls": [
      "i.imgur.com",
      "i.file.glass",
      "dropbox.com",
      "c.tenor.com",
      "discord.com",
      "cdn.discordapp.com",
      "media.discordapp.com",
      "upload.wikipedia.org",
      "i.projecterror.dev",
      "aurorahub.gg",
      "cdn.aurorahub.gg",
      "aurorav.net"
    ]
  },
  "profanityFilter": {
    "enabled": true,
    "badWords": ["tangina",
    "bobo",
    "idiot",
    "fuck",
    "fucking",
    "nigga",
    "pukunan",
    "pikunin",
    "tanga"]
  },
  "twitter": {
    "showNotifications": true,
    "generateProfileNameFromUsers": true,
    "allowEditableProfileName": true,
    "allowDeleteTweets": true,
    "allowReportTweets": true,
    "allowRetweet": true,
    "characterLimit": 160,
    "newLineLimit": 10,
    "enableAvatars": true,
    "enableEmojis": true,
    "enableImages": true,
    "maxImages": 5
  },
  "match": {
    "generateProfileNameFromUsers": true,
    "allowEditableProfileName": true
  },
  "marketplace": {
    "persistListings": false
  },
  "debug": {
    "level": "error",
    "enabled": true,
    "sentryEnabled": false
  },
  "defaultContacts": [],
  "disabledApps": [],
  "apps": [],
  "voiceMessage": {
    "enabled": true,
    "authorizationHeader": "PE-Secret",
    "url": "https://aurorahub.gg/rpp/imgs/upload.php",
    "returnedDataIndexes": ["url"]
  }
}
