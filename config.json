{
  "PhoneAsItem": {
    "enabled": true,
    "exportResource": "aurora_npwd",
    "exportFunction": "doesClientHavePhone"
  },
  "customPhoneNumber": {
    "enabled": true,
    "exportResource": "aurora_npwd",
    "exportFunction": "generateNumber"
  },
  "general": {
    "useResourceIntegration": true,
    "toggleKey": "f3",
    "toggleCommand": "phone",
    "defaultLanguage": "en"
  },
  "profanityFilter": {
    "enabled": true,
    "badWords": [
      "tangina",
      "bobo",
      "idiot",
      "fuck",
      "fucking",
      "nigga",
      "pukunan",
      "pikunin",
      "tanga"
    ]
  },
  "database": {
    "useIdentifierPrefix": true,
    "playerTable": "users",
    "identifierColumn": "identifier",
    "identifierType": "steam",
    "profileQueries": true,
    "phoneNumberColumn": "phone"
  },
  "images": {
    "url": "https://aurorahub.gg/rpp/imgs/upload.php",
    "type": "pe_image",
    "contentType": "multipart/form-data",
    "useAuthorization": false,
    "returnedDataIndexes": ["url"]
  },
  "imageSafety": {
    "filterUnsafeImageUrls": true,
    "embedUnsafeImages": false,
    "embedUrl": "https://i.example.com/embed",
    "safeImageUrls": [
      "i.imgur.com",
      "i.file.glass",
      "dropbox.com",
      "c.tenor.com",
      "discord.com",
      "cdn.discordapp.com",
      "media.discordapp.com",
      "upload.wikipedia.org",
      "i.projecterror.dev",
      "aurorahub.gg",
      "cdn.aurorahub.gg",
      "aurorav.net"
    ]
  },
  "debug": {
    "level": "error",
    "enabled": true,
    "sentryEnabled": false
  },
  "defaultContacts": [],
  "disabledApps": [],
  "apps": [],
  "voiceMessage": {
    "enabled": true,
    "authorizationHeader": "PE-Secret",
    "url": "https://aurorahub.gg/rpp/imgs/upload.php",
    "returnedDataIndexes": ["url"]
  }
}
